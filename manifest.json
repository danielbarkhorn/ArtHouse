{
	"name": "ArtHouse",
	"version": "0.0",
	"description": "Arty tabs",
	"manifest_version": 2,
	"background": {
    "persistent": false,
    "scripts": ["override/art.js", "override/background.js"]
  },
	"chrome_url_overrides": {
		"newtab": "override/newtab.html"
	},
	"permissions": [
    "storage",
		"unlimitedStorage",
		"https://www.artic.edu/iiif"
  ]
}
